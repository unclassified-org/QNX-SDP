<?xml version="1.0" encoding="UTF-8" ?>
<plugin name="%plugin.name" id="com.qnx.doc.qdb_en.dev_guide" version="1.0"
        provider-name="%plugin.providername">

    <extension point="org.eclipse.help.toc">
        <toc file="main.xml" primary="true"/>
    </extension>
        
    <extension point="org.eclipse.help.index">
        <index file="index.xml"/>
    </extension>
        
    <extension id="com.qnx.doc.qdb_en.dev_guide.libhover"
        name="QNX Neutrino QDB Documentation"
        point="org.eclipse.linuxtools.cdt.libhover.library">
        <library location="output.libhover" name="QNX Neutrino QDB Dev Guide Documentation" type="C" />
    </extension>
</plugin>
