// clocale standard header
#ifndef _CLOCALE_
#define _CLOCALE_
#include <yvals.h>

#ifdef _STD_USING
 #undef _STD_USING
  #include <locale.h>
 #define _STD_USING

#else /* _STD_USING */
 #include <locale.h>
#endif /* _STD_USING */

 #ifdef _GLOBAL_USING
_STD_BEGIN
using _CSTD lconv; using _CSTD localeconv; using _CSTD setlocale;
_STD_END
 #endif /* _GLOBAL_USING */

#endif /* _CLOCALE_ */

