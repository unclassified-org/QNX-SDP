#ifndef _CSIGNAL_
#define _CSIGNAL_
#include <yvals.h>

#ifdef _STD_USING
 #undef _STD_USING
  #include <signal.h>
 #define _STD_USING

#else /* _STD_USING */
 #include <signal.h>
#endif /* _STD_USING */

 #ifdef _GLOBAL_USING
_STD_BEGIN
using _CSTD sig_atomic_t; using _CSTD raise; using _CSTD signal;
_STD_END
 #endif /* _GLOBAL_USING */

#endif /* _CSIGNAL_ */

