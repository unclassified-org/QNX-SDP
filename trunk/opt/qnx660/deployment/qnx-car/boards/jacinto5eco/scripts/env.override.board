export GRAPHICS_ROOT="$CPUROOT/usr/lib/graphics/jacinto5"
export WK_DISPLAY=1
export RUN_RADIO=0

# Leaving the SATA driver in the list of apps to keep.  Has no effect on sd-only systems since this is just a list
